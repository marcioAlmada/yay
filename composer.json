{
    "name": "yay/yay",
    "description": "A high level PHP Pre-Processor",
    "license": "MIT",
    "keywords": [
        "pre-processor",
        "language",
        "syntax"
    ],
    "authors": [
        {
            "name": "MÃ¡rcio Almada",
            "email": "marcio3w@gmail.com",
            "homepage": "https://github.com/marcioAlmada"
        }
    ],
    "require": {
        "php": "7.*",
        "ext-mbstring": "*",
        "ext-tokenizer": "*",
        "nikic/php-parser": "^2.0"
    },
    "autoload": {
        "files": [
            "yay_parse.php",
            "yay_pretty.php",
            "src/parsers.php",
            "src/expanders.php",
            "bootstrap.php"
        ],
        "psr-4": {
            "Yay\\": "src/"
        }
    },
    "bin": [
        "bin/yay",
        "bin/yay-pretty"
    ],
    "require-dev": {
        "phpunit/phpunit": "~5.2"
    }
}
